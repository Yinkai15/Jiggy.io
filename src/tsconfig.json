{
    "compilerOptions": {
        "sourceMap": true,
        "noImplicitAny": true,
        "declaration": true,
        "moduleResolution": "node",
        "removeComments": true,
        "module": "commonjs",
        "target": "es5",
        "noImplicitReturns": true,
        "forceConsistentCasingInFileNames": true,
        "pretty": true,
        "allowUnreachableCode": false,
        "outDir": "../dist/lib/@jiggy",
        "baseUrl": "./",
        "paths": {
            "@jiggy/*" : ["./*"]
        }
    }
}