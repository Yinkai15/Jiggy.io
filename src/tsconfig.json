// This is a config purely used for development editors. Using this config to compile will not emit or compile files.

{
    "compilerOptions": {
        "sourceMap": true,
        "noImplicitAny": true,
        "declaration": true,
        "noEmit": true,
        "moduleResolution": "node",
        "removeComments": true,
        "module": "commonjs",
        "target": "es5",
        "noImplicitReturns": true,
        "forceConsistentCasingInFileNames": true,
        "pretty": true,
        "allowUnreachableCode": false
    }
}